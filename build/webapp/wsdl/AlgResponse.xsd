<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://schemas.hu.fnt.nl/berichten/alg_request">

	<xsd:annotation>
		<xsd:appinfo>generation request</xsd:appinfo>
		<xsd:documentation xml:lang="en">
			This Schema defines a
			iban generation request.
		</xsd:documentation>
	</xsd:annotation>

	<xsd:element name="alg-request">

		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="id" minOccurs="0" maxOccurs="1">
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:pattern value="[0-9]{1,10}"></xsd:pattern>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>

				<xsd:element name="access-token" minOccurs="1" maxOccurs="1" type="xsd:string"/>
				<xsd:element name="username" minOccurs="1" maxOccurs="1" type="xsd:string"/>
				<xsd:element name="remote-address" minOccurs="1" maxOccurs="1" type="xsd:string"/>

			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>



</xsd:schema>